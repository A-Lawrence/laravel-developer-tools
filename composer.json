{
    "name": "alawrence/laravel-dev-tools",
    "description": "Additional Laravel Development Tools for use locally.",
    "license": "GPL-3.0-only",
    "keywords": [
        "laravel",
        "lumen",
        "development tools",
        "devtools"
    ],
    "authors": [
        {
            "name": "Anthony Lawrence",
            "email": "artisan@anthonylawrence.me.uk"
        }
    ],
    "require": {
        "php": ">=7.0",
        "tightenco/collect": "^5.6"
    },
    "require-dev": {
        "phpspec/phpspec": "^4.3",
        "leanphp/phpspec-code-coverage": "^4.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "laracasts/generators": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "ALawrence\\LaravelDevTools\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "ALawrence\\LaravelDevTools\\ServiceProvider"
            ],
            "aliases": {
                "DevTools": "ALawrence\\LaravelDevTools\\Facade"
            }
        }
    }
}
